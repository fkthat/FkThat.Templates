<Project>
  <PropertyGroup>
    <VersionPrefix>0.0.0</VersionPrefix>
  </PropertyGroup>
  <!-- Build properties -->
  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <IsPackable>false</IsPackable>
    <Nullable>enable</Nullable>
    <NoWarn>CA1062;$(NoWarn)</NoWarn>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>
  <!-- Assembly and package properties -->
  <PropertyGroup>
    <Authors>FkThat</Authors>
    <Company>FkThat</Company>
    <Copyright>Â© FkThat, 2022</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <AssemblyVersion>$(VersionPrefix).0</AssemblyVersion>
    <FileVersion>$(VersionPrefix).0</FileVersion>
    <PackageProjectUrl>https://github.com/fkthat/FkThat.Templates.Solution</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/fkthat/FkThat.Templates.Solution</RepositoryUrl>
    <RepositoryBranch>$(RepositoryBranch)</RepositoryBranch>
    <RepositoryCommit>$(RepositoryCommit)</RepositoryCommit>
    <PackageTags></PackageTags>
    <Description></Description>
    <PackageReleaseNotes>@(ReleaseNoteLines, '%0a')</PackageReleaseNotes>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>
  <!-- Release notes from the file -->
  <Target Name="PreparePackageReleaseNotesFromFile" BeforeTargets="GenerateNuspec">
    <ReadLinesFromFile File="$(MSBuildThisFileDirectory)RELEASE-NOTES" >
      <Output TaskParameter="Lines" ItemName="ReleaseNoteLines"/>
    </ReadLinesFromFile>
  </Target>
</Project>
