<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <!--#if(target-framework == '31')-->
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <!--#endif-->
    <!--#if(target-framework == '50')-->
    <TargetFramework>net5.0</TargetFramework>
    <!--#endif-->
    <IsPackable>false</IsPackable>
    <!--#if(target-project != '')-->
    <RootNamespace>__TargetProject</RootNamespace>
    <!--#endif-->
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <NoWarn>CA1062;$(NoWarn)</NoWarn>
  </PropertyGroup>
  <!--#if(target-project != '')-->
  <PropertyGroup>
    <TargetProjectPath>..\..\src\__TargetProject\__TargetProject.csproj</TargetProjectPath>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="$(TargetProjectPath)" Condition="Exists($(TargetProjectPath))" />
  </ItemGroup>
  <!--#endif-->
  <PropertyGroup>
    <CollectCoverage>true</CollectCoverage>
    <CoverletOutputFormat>cobertura</CoverletOutputFormat>
    <!--#if(target-project != '')-->
    <Include>[__TargetProject]*</Include>
    <!--#endif-->
    <Exclude>[*]*.Program,[*]*.Startup</Exclude>
    <ExcludeByAttribute>Obsolete,GeneratedCodeAttribute,CompilerGeneratedAttribute</ExcludeByAttribute>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="coverage.cobertura.xml" />
  </ItemGroup>
</Project>
